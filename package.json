{
  "name": "commercial-coding-backend",
  "version": "1.0.0",
  "type": "module",
  "description": "Backend service for Commercial Coding",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "deploy": "chmod +x ./deploy.sh && ./deploy.sh",
    "deploy:push": "chmod +x ./deploy.sh && ./deploy.sh yes",
    "test:sheets": "node test-google-sheets.js",
    "test:newsletter": "node test-newsletter-endpoint.js",
    "test:booking": "node test-booking.js",
    "test:contact": "node test-contact-form.js",
    "test:email": "node test-mailersend.js",
    "test:email-attachment": "node test-mailersend-attachment.js"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "googleapis": "^126.0.1",
    "ical-generator": "^4.1.0",
    "mailersend": "^2.2.0",
    "node-fetch": "^3.3.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "private": true,
  "engines": {
    "node": ">=18.0.0"
  },
  "notes": {
    "deployment": "The deploy.sh script is not included in the repository. Please obtain it separately and place it in the project root."
  }
}

